#!/usr/bin/python3.5
# EASY-INSTALL-ENTRY-SCRIPT: 'pwntools==2.2.1','console_scripts','elfpatch'
__requires__ = 'pwntools==2.2.1'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pwntools==2.2.1', 'console_scripts', 'elfpatch')()
    )
