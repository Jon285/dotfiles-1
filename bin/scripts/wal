#!/usr/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'pywal==1.3.3','console_scripts','wal'
__requires__ = 'pywal==1.3.3'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pywal==1.3.3', 'console_scripts', 'wal')()
    )
